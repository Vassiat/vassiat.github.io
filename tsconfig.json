{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "esModuleInterop": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "strict": false,
    "rootDir": ".",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
    },
    "noEmit": true
  },
}
